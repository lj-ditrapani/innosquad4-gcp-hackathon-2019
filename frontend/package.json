{
    "name": "frontend",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@types/jest": "24.0.15",
        "@types/node": "12.6.8",
        "@types/react": "16.8.23",
        "@types/react-dom": "16.8.4",
        "react": "^16.8.6",
        "react-dom": "^16.8.6",
        "react-scripts": "3.0.1",
        "typescript": "3.5.3"
    },
    "scripts": {
        "fmt": "prettier --write \"*.json\" \"src/**/*.{ts,tsx}\"",
        "fmt-check": "prettier --check \"*.json\" \"src/**/*.{ts,tsx}\"",
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "all": "npm run fmt && npm run build",
        "all-pipeline": "npm run fmt-check && npm run build"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "prettier": "^1.18.2"
    }
}
